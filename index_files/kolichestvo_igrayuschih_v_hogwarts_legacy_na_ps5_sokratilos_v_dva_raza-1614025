      <script>
      let navRegTrig = document.querySelector('.user-panel-area .signup-button');
      let postRegTrig = document.querySelector('.user-panel-area [data-balloon="Добавить новость, мнение, файл..."]');
      let downloadRegTrig = document.querySelectorAll('.js-download-btn-set a[href="#signinModalWindow"]');
      let postRatingRegTrig = document.querySelectorAll('#postRating a');
      let commentRatingRegTrig = document.querySelectorAll('#comments-container .js-comment-item-rating-btn');
      let replyRegTrig = document.querySelectorAll('#comments-container .js-reply-btn');
      let commentNewRegTrig = document.querySelector('#fake-comment-editor');
      let subscribeRegTrig = document.querySelectorAll('.gp-game-follow');
      let prizeRegTrig = document.querySelector('.prize-win-container button');

      navRegTrig.addEventListener('click', function() {
        localStorage.setItem('regTrigger', 'Навигация');
        localStorage.setItem('regSource', window.location.pathname);
      });
      postRegTrig.addEventListener('click', function() {
        localStorage.setItem('regTrigger', 'Новый пост');
        localStorage.setItem('regSource', window.location.pathname);
      });
      for(let i = 0; i < downloadRegTrig.length; i++) {
        downloadRegTrig[i].addEventListener('click', function() {
          localStorage.setItem('regTrigger', 'Скачивание');
          localStorage.setItem('regSource', window.location.pathname);
        });
      }
      for(let i = 0; i < postRatingRegTrig.length; i++) {
        postRatingRegTrig[i].addEventListener('click', function() {
          localStorage.setItem('regTrigger', 'Рейтинг поста');
          localStorage.setItem('regSource', window.location.pathname);
        });
      }
      for(let i = 0; i < replyRegTrig.length; i++) {
        replyRegTrig[i].addEventListener('click', function() {
          localStorage.setItem('regTrigger', 'Ответ на комментарий');
          localStorage.setItem('regSource', window.location.pathname);
        });
      }
      for(let i = 0; i < commentRatingRegTrig.length; i++) {
        commentRatingRegTrig[i].addEventListener('click', function() {
          localStorage.setItem('regTrigger', 'Рейтинг комментария');
          localStorage.setItem('regSource', window.location.pathname);
        });
      }
      if (commentNewRegTrig) {
        commentNewRegTrig.addEventListener('click', function() {
        localStorage.setItem('regTrigger', 'Новый комментарий');
        localStorage.setItem('regSource', window.location.pathname);
      });
      }
      for(let i = 0; i < subscribeRegTrig.length; i++) {
        subscribeRegTrig[i].addEventListener('click', function() {
          localStorage.setItem('regTrigger', 'Подписка на игру');
          localStorage.setItem('regSource', window.location.pathname);
        });
      }
      if (prizeRegTrig) {
        prizeRegTrig.addEventListener('click', function() {
        localStorage.setItem('regTrigger', 'Выиграть приз');
        localStorage.setItem('regSource', window.location.pathname);
      });
      }
    </script>
